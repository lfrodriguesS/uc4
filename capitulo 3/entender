#include<stdio.h> 
int main(){ 

   int nume; 

   printf("digite um numero:"); 
   scanf("%d", &nume);  
   
   if ( nume < 0 ){ 
            printf(" num menor q nega:"); 
   }else if( nume > 0 ){ 
            printf(" num maior que posi"); 
   }else (nume = 0){
    
   }


}
   


   

    






